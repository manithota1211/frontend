apiVersion: v1
kind: Pod
metadata:
  name: my-nginx-pod
  labels:
    front: app
spec:
  containers: 
    - image: nginx:latest
      name: backend-container
      ports:
        - containerPort: 80
      
---

apiVersion: v1
kind: Pod
metadata:
  name: env-pod
  labels:
    front: app
spec:
  containers:
    - image: nginx:latest
      name: backend-nginx
      ports:
        - containerPort: 80

      env:
        - name: ENV
          value: dev

---
